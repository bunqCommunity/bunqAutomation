{
    "compilerOptions": {
        "outDir": "build-server",
        "rootDir": "server",

        "moduleResolution": "node",
        "target": "es6",
        "module": "commonjs",

        "sourceMap": false,
        "declaration": true,
        "noImplicitAny": false,
        "removeComments": true,
        "resolveJsonModule": true
    }
}
