{
    "name": "bunq-automation",
    "version": "0.1.0",
    "repository": "git@github.com:bunqCommunity/bunqAutomation.git",
    "author": "Gregory Goijaerts <bunqcommunity@gmail.com>",
    "license": "MIT",
    "scripts": {
        "start": "node build-server/Server.js",
        "pm2": "pm2 start pm2.yml",
        "build": "yarn run build:server && yarn run build:client",
        "build:server": "./node_modules/.bin/babel server --out-dir build-server",
        "build:client": "react-scripts build",
        "postbuild:client": "./node_modules/.bin/sw-precache --config src/ServiceWorker/sw-precache-config.js",
        "server": "./node_modules/.bin/nodemon --config .nodemonrc.json --exec babel-node server/Server.js",
        "react": "react-scripts start",
        "dev": "./node_modules/.bin/concurrently \"yarn:server\" \"yarn:react\" --names Server,React",
        "postinstall": "node scripts/install.js",
        "test": "./node_modules/.bin/jest --config jest.config.js --no-cache --verbose",
        "test:dev": "./node_modules/.bin/jest --config jest.config.js --watch",
        "prettier": "./node_modules/.bin/prettier --write \"+(tests|src|server)/**/**.+(ts|js|jsx|scss|css)\""
    },
    "dependencies": {
        "@bunq-community/bunq-js-client": "^0.40.0",
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
        "axios": "^0.18.0",
        "chalk": "^2.4.1",
        "classnames": "^2.2.6",
        "dotenv": "^6.2.0",
        "fastify": "^2.1.0",
        "fastify-auth": "^0.5.0",
        "fastify-compress": "^0.8.1",
        "fastify-cors": "^2.1.2",
        "fastify-helmet": "^3.0.0",
        "fastify-response-time": "^1.1.0",
        "fastify-static": "^2.3.4",
        "fastify-swagger": "^2.3.2",
        "glob": "^7.1.3",
        "json-store": "^1.0.0",
        "level": "^4.0.0",
        "level-js": "^4.0.0",
        "levelup": "^4.0.0",
        "node-sass": "^4.11.0",
        "pino-pretty": "^2.5.0",
        "pm2": "^3.4.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-helmet": "^5.2.0",
        "react-router": "^5.0.0",
        "react-router-dom": "^5.0.0",
        "react-scripts": "^2.1.8",
        "redux": "^4.0.1",
        "redux-logger": "^3.0.6",
        "redux-react-hook": "^3.2.1",
        "redux-thunk": "^2.3.0",
        "socket.io": "^2.2.0",
        "socket.io-client": "^2.2.0",
        "storm-react-diagrams": "^5.2.1",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-async-to-generator": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/preset-react": "^7.0.0",
        "babel-plugin-dynamic-import-node": "^2.2.0",
        "concurrently": "^4.1.0",
        "nodemon": "^1.18.10",
        "prettier": "^1.15.3",
        "sw-precache": "^5.2.1"
    },
    "browserslist": [
        "last 1 version",
        "> 1%",
        "maintained node versions",
        "not dead"
    ]
}
