    "license": "MIT",
    "scripts": {
        "start": "nodemon -L --exec babel-node server/Server.js",
        "serve": "node build-server/Server.js",
        "build": "babel server --out-dir build-server",
        "certs": "node server/Scripts/generate-certs.js",
        "prettier": "./node_modules/.bin/prettier --tab-width 4 --print-width 120 --write \"+(tests|src)/**/**.+(js|jsx|scss)\""
    },
    "dependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/plugin-transform-async-to-generator": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "dotenv": "^6.2.0",
        "fastify": "^1.13.3",
        "fastify-compress": "^0.7.1",
        "fastify-helmet": "^3.0.0",
        "fastify-response-time": "^1.1.0",
        "fastify-swagger": "^1.1.0",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "nodemon": "^1.18.10",
        "prettier": "^1.15.3",
        "selfsigned": "^1.10.4"
    }
