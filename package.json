{
    "name": "bunq-automation",
    "version": "0.1.0",
    "repository": "git@github.com:bunqCommunity/bunqAutomation.git",
    "author": "Gregory Goijaerts <bunqcommunity@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "yarn run build:server && yarn run build:client",
        "start": "nodemon -L --exec babel-node server/Server.js",
        "serve": "node build-server/Server.js",
        "build:server": "babel server --out-dir build-server",
        "react": "react-scripts start",
        "build:client": "react-scripts build",
        "certs": "node server/Scripts/generate-certs.js",
        "prettier": "./node_modules/.bin/prettier --tab-width 4 --print-width 120 --write \"+(tests|src)/**/**.+(js|jsx|scss)\""
    },
    "dependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
        "@babel/plugin-transform-async-to-generator": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@bunq-community/bunq-js-client": "^0.40.0",
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
        "chalk": "^2.4.1",
        "dotenv": "^6.2.0",
        "fastify": "^2.1.0",
        "fastify-compress": "^0.8.1",
        "fastify-helmet": "^3.0.0",
        "fastify-response-time": "^1.1.0",
        "fastify-static": "^2.3.4",
        "fastify-swagger": "^2.3.2",
        "json-store": "^1.0.0",
        "node-sass": "^4.11.0",
        "react": "^16.7.0-alpha.2",
        "react-dom": "^16.7.0-alpha.2",
        "react-helmet": "^5.2.0",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^2.1.8",
        "react-use": "^5.1.1",
        "storm-react-diagrams": "^5.2.1",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "nodemon": "^1.18.10",
        "prettier": "^1.15.3",
        "selfsigned": "^1.10.4"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
